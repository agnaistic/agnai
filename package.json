{
  "name": "agnaistic",
  "version": "0.1.0",
  "private": true,
  "description": "Agnostic AI Chat",
  "keywords": [],
  "license": "AGPL-3.0",
  "scripts": {
    "build": "NODE_ENV=production parcel build web/index.html --no-optimize --detailed-report",
    "build:prod": "NODE_ENV=production parcel build web/index.html --detailed-report",
    "build:win": "setx NODE_ENV \"production\" && parcel build web/index.html --no-optimize --detailed-report",
    "build:all": "npm run build && npm run build:server",
    "build:all:win": "npm run build:win && npm run build:server",
    "build:server": "tsc -p srv.tsconfig.json",
    "build:server:watch": "tsc -p srv.tsconfig.json -w",
    "format": "prettier web --check && prettier srv --check",
    "format:fix": "prettier web --write && prettier srv --write",
    "model": "python -m flask --app model/app.py run --host 0.0.0.0 -p 5001",
    "model:watch": "nodemon -e py --watch model --exec \"python -m flask --app model/app.py run --host 0.0.0.0 -p 5001\"",
    "server": "node srv/start.js",
    "server:watch": "nodemon -e js --delay 1 --watch srv --exec \"node --inspect srv/start.js\"",
    "start:all": "NODE_ENV=dev concurrently -c auto -n web,api,mod,tsc \"parcel web/index.html --no-cache\" \"npm run server:watch\" \"npm run model:watch\" \"npm run build:server:watch\"",
    "start": "NODE_ENV=dev concurrently -c auto -n web,api,tsc \"parcel web/index.html --no-cache\" \"npm run server:watch\" \"npm run build:server:watch\"",
    "start:public": "NODE_ENV=dev concurrently -c auto -n web,api,mod,lt \"parcel watch web/index.html --no-cache\" \"npm run server:watch\" \"npm run model:watch\" \"npx lt --port 3001\"",
    "start:public:win": "setx NODE_ENV \"dev\" && concurrently -c auto -n web,api,mod,lt \"parcel watch web/index.html --no-cache\" \"npm run server:watch\" \"npm run model:watch\" \"npx lt --port 3001\"",
    "start:win": "set NODE_ENV=dev && concurrently -c auto -n web,api,tsc \"parcel web/index.html --no-cache\" \"npm run server:watch\" \"npm run build:server:watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json",
    "up": "docker compose -p agnai up -d"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@solidjs/router": "^0.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/multiparty": "^0.0.33",
    "@types/nedb": "^1.8.12",
    "@types/needle": "^3.2.0",
    "@types/node": "^18.13.0",
    "@types/showdown": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.4",
    "babel-preset-solid": "^1.6.9",
    "bcryptjs": "^2.4.3",
    "buffer": "^5.5.0",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "frisker": "^1.6.0",
    "gpt-3-encoder": "^1.1.4",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "localtunnel": "^2.0.2",
    "lucide-solid": "0.105.0-alpha.9",
    "mkdirp": "^2.1.4",
    "mongodb": "^5.1.0",
    "multiparty": "^4.2.3",
    "needle": "^3.2.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.3",
    "pino": "^8.10.0",
    "pino-pretty": "^9.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "process": "^0.11.10",
    "redis": "^4.6.5",
    "showdown": "^2.1.0",
    "solid-js": "^1.6.9",
    "solid-refresh": "^0.4.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "ws": "^8.12.1",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "path-browserify": "^1.0.0",
    "png-chunk-text": "^1.0.0",
    "png-chunks-encode": "^1.0.0",
    "png-chunks-extract": "^1.0.0"
  }
}
